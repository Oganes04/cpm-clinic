#content .content {
	width: 50%;
	float: left;
}
#content .cont-text {
	padding-right: 60px;
}
#content .modal_block {
	max-width: 100%;
	padding: 0;
}
#content .modal_block .contentTitle {
	text-align: left;
}

@media (max-width: 991px) {
 #content .content {
	width: 100%;
 } 
 #content .cont-text {
	padding-right: 0px;
 }
}/*@media*/

@media (max-width: 479px) { 
 #content .modal_block .f_object#f_name {
	padding-right: 0px;
	width: 100%;
 }  
 #content .modal_block .f_object#f_phone {
	padding-left: 0px;
	width: 100%;
 }
}/*@media*/
